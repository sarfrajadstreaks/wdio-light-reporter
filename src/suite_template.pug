doctype html
html
  head 
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    title Light Reporter
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js")
    script(src="https://www.gstatic.com/charts/loader.js")
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet")
    link(rel="icon" type="image/x-icon" href="data:image/vnd.microsoft.icon;base64,")
    style(type="text/css").
      .card-header {
        background-color: inherit;
        border-bottom:0
      }
      .list-group-item.active {
        color:#000 !important;
        background-color: #dde3eb !important;
        border-color: #b4b6b9 !important
      }
    script.
      function generatePieChat(val,location,isRoot){
        var piechartData=[["Status", "per Run"],["Passed", val.passes],["Failed", val.failures],["Skipped", val.skipped]];
        google.charts.load("current", { packages: ["corechart"] });
        google.charts.setOnLoadCallback(drawChart);
        function drawChart() {
          var data = google.visualization.arrayToDataTable(piechartData);

          var options;
          if(isRoot){
            options= {
            title: '#{suiteName}',
            pieHole: 0.4,
            legend: "none",
            colors: ["#75ffac", "#ff7575", "#75d3ff"],
            };
          }else{
            options= {
            title: '#{suiteName}',
            legend: "none",
            colors: ["#75ffac", "#ff7575", "#75d3ff"],
            pieSliceText: "label",
            slices: {
                1: { offset: 0.3 },
                2: { offset: 0.2 },
              },
            };
          }
            
          var chart = new google.visualization.PieChart(location);
          chart.draw(data, options);
        }
      }
      function generateBarChart(val,runs,location,isRoot){
        var barGraphData=[["Name","Passed",{ role: "style" },"Failed", { role: "style" },"Skipped",{ role: "style" },{ role: "annotation" }]];
        if(isRoot){
          val.envs.map((d)=>{
          let env=d.split(",")
          barGraphData.push(
            [d,runs[env[2]][env[3]!=undefined?env[3]:"default"][env[0]][env[1]].stats.passes,"#75ffac",runs[env[2]][env[3]!=undefined?env[3]:"default"][env[0]][env[1]].stats.failures,
            "#ff7575",
            runs[env[2]][env[3]!=undefined?env[3]:"default"][env[0]][env[1]].stats.skipped,
            "#75d3ff",
            ''])
          })
        }else{
          runs.map(s=>{
             barGraphData.push([s.title,s.passes.length,"#75ffac",s.failures.length,"#ff7575",s.skipped.length,"#75d3ff",''])
          })
        }
        google.charts.load("current", { packages: ["corechart"] });
        google.charts.setOnLoadCallback(drawChart);
        function drawChart() {
          var data = google.visualization.arrayToDataTable(barGraphData);
          var options = {
            title: '#{suiteName}',
            tooltip: { textStyle: { fontName: '"Arial"' } },
            legend: "none",
            bar: { groupWidth: "75%" },
            isStacked: true,
            colors: ["#75ffac", "#ff7575", "#75d3ff"],
          };
          var chart = new google.visualization.ColumnChart(location);
          chart.draw(data, options);
        }
      }

    
  body#page-top
    nav.navbar.navbar-expand-sm.navbar-dark.shadow-sm.p-3.mb-2(style={background:"cadetblue"})
      div.container-fluid
        ul.navbar-nav
          li.nav-item
            a( href="#").nav-link.active
              img(src="data:image/png;base64," alt="cool_logo")
          li.nav-item
            a( href="#").nav-link.active= 'Light Reporter : '+ suiteName
    

    div.row(style={"min-height":"88vh"})
      div.col-sm-10.mx-auto
        div.row.no-gutters.mt-3
          div.col-sm-2.no-gutters
            div.card(style={width:"100%", height:"200px","align-items": "center","justify-content": "space-evenly"})
              div.card-body(style={"flex": "0 1 auto"}) Tests: #{stats.tests}
          div.col-sm-2
            div.card(style={width:"100%", height:"200px","align-items": "center","justify-content": "space-evenly"})
              div.card-body(style={"flex": "0 1 auto"}) Passed: #{stats.passes}
          div.col-sm-2
            div.card(style={width:"100%", height:"200px","align-items": "center","justify-content": "space-evenly"})
              div.card-body(style={"flex": "0 1 auto"}) Failed: #{stats.failures}
          div.col-sm-2
            div.card(style={width:"100%", height:"200px","align-items": "center","justify-content": "space-evenly"})
              div.card-body(style={"flex": "0 1 auto"}) Skipped: #{stats.skipped}
          div.col-sm-2
            div.card
              div.card-body(style={padding:0})
                div#pie_chart_div
                  script.
                    generatePieChat(!{JSON.stringify(stats)},document.getElementById("pie_chart_div"),true);
          div.col-sm-2
            div.card
              div.card-body(style={padding:0})
                div#bar_chart_div
                  script.
                    generateBarChart(!{JSON.stringify(stats)},!{JSON.stringify(runs)},document.getElementById("bar_chart_div"),true);
        div.row.no-gutters.mt-3
          div.accordion#accordionFlushExample
            each val,i in stats.envs 
              - var env=val.split(",")
              div.accordion-item
                h6.accordion-header(id="flush-heading"+i)
                - var platform 
                  if env[2]==='linux'
                    span(style={padding:"5px"}) <i class="fa-brands fa-linux" title= #{env[2]}></i>
                  else if env[2]==='window'
                    span(style={padding:"5px"}) <i class="fa-brands fa-windows" title= #{env[2]}></i>
                  else if env[2]==='macOS'
                    span(style={padding:"5px"}) <i class="fa-brands fa-apple" title= #{env[2]}></i>
                  else if env[2]==='android'
                    span(style={padding:"5px"}) <i class="fa-brands fa-android" title= #{env[2]}></i>
                  else 
                    span(style={padding:"5px"}) <i class="fa-solid fa-lightbulb" title= #{env[2]}></i>

                  span(style={padding:"5px"}) <i class="fa-solid fa-angles-right"></i>
                - var browser
                  if env[0]==='chrome'
                    span(style={padding:"5px"}) <i class="fa-brands fa-chrome" title=#{env[0]}></i> 
                  else if env[0]==='firefox'
                    span(style={padding:"5px"}) <i class="fa-brands fa-firefox-browser" title=#{env[0]}></i>
                  else if env[0]==='msedge'
                    span(style={padding:"5px"}) <i class="fa-brands fa-edge" title=#{env[0]}></i>
                  else if env[0]==='safari'
                    span(style={padding:"5px"}) <i class="fa-brands fa-safari" title=#{env[0]}></i>
                  else 
                    span(style={padding:"5px"}) <i class="fa-solid fa-lightbulb" title=#{env[0]}></i>
                  
                  span(style={padding:"5px"}) <i class="fa-solid fa-angles-right"></i> 
                  span(style={padding:"5px"}) #{env[1]!==undefined?env[1].replace(/_/g,"."):env[1]}

                  div.accordion-button.collapsed(type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapse"+i aria-expanded="false" aria-controls="flush-collapse"+i)
                    div.col-sm-2.no-gutters
                      div.card(style={width:"100%", height:"100px","align-items": "center","justify-content": "space-evenly"})
                        div.card-body(style={"flex": "0 1 auto"}) Tests: #{runs[env[2]][env[3]!=undefined?env[3]:'default'][env[0]][env[1]].stats.tests}
                    div.col-sm-2
                      div.card(style={width:"100%", height:"100px","align-items": "center","justify-content": "space-evenly"})
                        div.card-body(style={"flex": "0 1 auto"}) Passed: #{runs[env[2]][env[3]!=undefined?env[3]:'default'][env[0]][env[1]].stats.passes}
                    div.col-sm-2
                      div.card(style={width:"100%", height:"100px","align-items": "center","justify-content": "space-evenly"})
                        div.card-body(style={"flex": "0 1 auto"}) Failed: #{runs[env[2]][env[3]!=undefined?env[3]:'default'][env[0]][env[1]].stats.failures}
                    div.col-sm-2
                      div.card(style={width:"100%", height:"100px","align-items": "center","justify-content": "space-evenly"})
                        div.card-body(style={"flex": "0 1 auto"}) Skipped: #{runs[env[2]][env[3]!=undefined?env[3]:'default'][env[0]][env[1]].stats.skipped}
                    div.col-sm-2
                      div.card
                        div.card-body(style={padding:0})
                          div(id="pie_chart_div_"+i , style={width:"100%", height:"100px"})
                            script.
                              var locator="#{val}".split(",");
                              var data=!{JSON.stringify(runs)}[locator[2]][locator[3]!=undefined?locator[3]:"default"][locator[0]][locator[1]].stats
                              generatePieChat(data, document.getElementById('pie_chart_div_'+#{i}));
                    div.col-sm-2
                      div.card
                        div.card-body(style={padding:0})
                          div(id="bar_chart_div_"+i , style={width:"100%", height:"100px"})
                            script.
                              var locator="#{val}".split(",");
                              var data=!{JSON.stringify(runs)}[locator[2]][locator[3]!=undefined?locator[3]:"default"][locator[0]][locator[1]].scenarios
                              generateBarChart(null,data, document.getElementById('bar_chart_div_'+#{i}),false);

                div.accordion-collapse.collapse(id="flush-collapse"+i aria-labelledby="flush-heading"+i data-bs-parent="#accordionFlushExample")
                  div.accordion-body(style={display: "flex"})
                    div.col-sm-6
                      div.card
                        div.card-body
                          h5.card-title Scenarios
                          ul.list-group(id="sc_list")
                            each val in runs[env[2]][env[3]!=undefined?env[3]:'default'][env[0]][env[1]].scenarios
                              li.list-group-item.text-truncate(id="sc_"+val.uuid) !{val.title}
                                span.badge.bg-secondary.float-end #{val.duration} ms
                                div.progress
                                  div.progress-bar.progress-bar-striped.bg-success(role="progressbar" style="width: "+(val.passes.length/val.tests.length)*100+"%"  aria-valuemax="100")=(val.passes.length/val.tests.length)*100+ "%"
                                  div.progress-bar.progress-bar-striped.bg-danger(role="progressbar" style="width: "+(val.failures.length/val.tests.length)*100+"%" aria-valuemax="100")=(val.failures.length/val.tests.length)*100+"%"
                                  div.progress-bar.progress-bar-striped.bg-info(role="progressbar" style="width: "+(val.skipped.length/val.tests.length)*100+"%"  aria-valuemax="100")=(val.skipped.length/val.tests.length)*100+"%"
                                  div.collapse(id="sc_"+val.uuid)
                                    //- if !{val.context}!=''
                                    div.card.card-body.text-wrap !{val.context}
                                      
                              

                    div.col-sm-6
                      div.card
                        div.card-body
                          h5.card-title TestCases
                          div.accordion#accordionExample
                            each sc,i in runs[env[2]][env[3]!=undefined?env[3]:'default'][env[0]][env[1]].scenarios
                              each ts in sc.tests
                                div.accordion-item.d-none(scenarios="sc_"+sc.uuid)
                                    h2.accordion-header(id="heading_"+ts.uuid)
                                      if ts.state=="failed"
                                        button.accordion-button.collapsed.alert-danger(type="button" data-bs-toggle="collapse" data-bs-target="#collapse_"+ts.uuid aria-expanded="false" aria-controls="collapse_"+ts.uuid) !{ts.title}
                                      else if ts.state=="skipped"
                                        button.accordion-button.collapsed.alert-info(type="button" data-bs-toggle="collapse" data-bs-target="#collapse_"+ts.uuid aria-expanded="false" aria-controls="collapse_"+ts.uuid) !{ts.title}
                                      else 
                                        button.accordion-button.collapsed.alert-success(type="button" data-bs-toggle="collapse" data-bs-target="#collapse_"+ts.uuid aria-expanded="false" aria-controls="collapse_"+ts.uuid) !{ts.title}
                                    div.accordion-collapse.collapse(id="collapse_"+ts.uuid aria-labelledby="heading_"+ts.uuid data-bs-parent="#accordionExample")
                                      div.accordion-body
                                        if ts.state==="failed"
                                          div.alert.alert-danger(role="alert")= ts.err.stack
                                        each val,i in JSON.parse(ts.context)
                                          if typeof val=="object"
                                            if val.title==="Screenshot"
                                              img(src=val.value, style={width:" -webkit-fill-available",cursor: "pointer"}, data-bs-toggle="modal",data-bs-target="#Modal_"+ts.uuid)
                                              div.modal.fade(id="Modal_"+ts.uuid,tabindex="-1" aria-labelledby="ModalLabel_"+ts.uuid aria-hidden="true")
                                                div.modal-dialog.modal-xl
                                                  div.modal-content
                                                    div.modal-body
                                                      img(src=val.value,style={width:" -webkit-fill-available"})

                                          else 
                                             div 
                                              span(style={color: "green",padding:"2px" }) &#10003;
                                              span= "  "+val
      
    div.row
        div.card
          small(style={"padding-left":"5%","font-size": "8px"}) Copyright @ #{copyright}, <i class="fa-brands fa-github"></i> <a href='#{developer}'>sarfrajadstreaks</a>
  script.
    var scenarios=document.querySelectorAll('#sc_list');
    scenarios.forEach(scenario=>{
      var sc_list= Array.prototype.slice.call(scenario.children);
      sc_list.forEach((ele)=>{
        sc_list[0].className=sc_list[0].className+" active"
        document.querySelectorAll(`[scenarios="${sc_list[0].id}"]`).forEach((ts)=>{
            ts.className='accordion-item'
          })
        ele.addEventListener("click", function () {
        for(let i=0;i<sc_list.length;i++){
          sc_list[i].className="list-group-item text-truncate"
          document.querySelectorAll(`[scenarios="${sc_list[i].id}"]`).forEach((ts)=>{
            ts.className+=" d-none"
          })
        }
        this.className=this.className+ " active"
        document.querySelectorAll(`[scenarios="${this.id}"]`).forEach((ts)=>{
            ts.className='accordion-item'
          })
        })
      })
    })
   